         // (bug 514670)
         if (borderRadii[C_TL].width > 0 || borderRadii[C_BL].width > 0) {
           borderSizes[NS_SIDE_LEFT] = spreadDistance;
         }
 
         if (borderRadii[C_TL].height > 0 || borderRadii[C_TR].height > 0) {
           borderSizes[NS_SIDE_TOP] = spreadDistance;
