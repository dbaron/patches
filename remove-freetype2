-  nsCompressedCharMap ccmapObj;
-  if (aFce->mFlags & FCE_FLAGS_SURROGATE) {
-    ccmapObj.Extend();
-  }
-  ccmapObj.SetChars(aFce->mCCMap);
-  return ccmapObj.NewCCMap();
-}
