-{
-  FtFuncList *p;
-  void *ptr = this;
-  for (p=FtFuncs; p->FuncName; p++) {
-    *((PRFuncPtr*)((char*)ptr+p->FuncOffset)) = 
-                        (PRFuncPtr)&nsFreeType2__DummyFunc;
-  }
-}
