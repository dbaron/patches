-#define TT_OS2_CPR2_ARABIC       (0x00080000) /* Arabic                      */
-#define TT_OS2_CPR2_CA_FRENCH    (0x00100000) /* MS-DOS Canadian French      */
-#define TT_OS2_CPR2_HEBREW       (0x00200000) /* Hebrew                      */
