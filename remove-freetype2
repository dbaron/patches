-  if (!sCharSetManager) {
-    //
-    // get the sCharSetManager
-    //
-    CallGetService(NS_CHARSETCONVERTERMANAGER_CONTRACTID, &sCharSetManager);
-    NS_ASSERTION(sCharSetManager,"failed to create the charset manager");
-  }
-  return sCharSetManager;
-}
